@model IEnumerable<SIMS_Demo.Models.Course>

<h1>Courses</h1>

<p>Hello @ViewBag.UserName, you are @ViewBag.UserRole</p>

<table class="table">
    <thead>
        <tr>
            <th>Subject</th>
            <th>Day</th>
            <th>Slot</th>
            <th>Room</th>
            <th>Teacher</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var course in Model)
        {
            <tr>
                <td>@course.Subject</td>
                <td>@course.Time</td>
                <td>@course.Slot</td>
                <td>@course.RoomNumber</td>
                <td>@course.Teacher</td>
                <td>
                    
                </td>
            </tr>
        }
    </tbody>
</table>


