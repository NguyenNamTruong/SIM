<p>Hello @ViewBag.UserName, you are @ViewBag.UserRole</p>

<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Slot</th>
            <th>Student Group</th>
            <th>Instructor</th>
            <th>Course</th>
            <th>Session Number</th>
            <th>Campus/Programme</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var timetable in Model)
        {
            <tr>
                <td>@timetable.Date.ToString("dddd dd/MM/yyyy")</td>
                <td>@timetable.Slot</td>
                <td>@timetable.StudentGroup</td>
                <td>@timetable.Instructor</td>
                <td>@timetable.CourseName</td>
                <td>@timetable.SessionNumber</td>
                <td>@timetable.CampusProgramme</td>
                <td>
                    <a class="btn btn-sm btn-primary" href="@Url.Action("Detail", "Timetable", new { id = timetable.Id })">Detail</a>
                </td>
            </tr>
        }
    </tbody>
</table>
