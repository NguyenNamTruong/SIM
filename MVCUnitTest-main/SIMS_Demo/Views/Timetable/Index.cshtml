@model IEnumerable<SIMS_Demo.Models.Timetable>

<h1>Timetable</h1>

<p>Hello @ViewBag.UserName, you are @ViewBag.UserRole</p>
<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Start Time</th>
            <th>End Time</th>
            <th>Location</th>
            <th>Description</th>
            <th>Organizer</th>
            <th>Status</th>
            <th>Action</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Title</td>
            <td>@item.StartTime.ToString("dd/MM/yyyy HH:mm")</td>
            <td>@item.EndTime.ToString("dd/MM/yyyy HH:mm")</td>
            <td>@item.Location</td>
            <td>@item.Description</td>
            <td>@item.Organizer</td>
            <td>@item.Status</td>
            <td>
                <a class="btn btn-sm btn-primary"
                   href="@Url.Action("Detail","Timetable",new {id=item.Id})">Detail</a>
                
            </td>
        </tr>
    }
</table>


